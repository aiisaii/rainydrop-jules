{
  "manifest_version": 3,
  "name": "RainyDrop Clone",
  "version": "1.0",
  "description": "A browser extension for the RainyDrop Clone.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
